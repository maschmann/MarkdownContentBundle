{
    "name": "asm/markdown-content-bundle",
    "description": "Symfony2 bundle for static markdown-file content management",
    "keywords": ["symfony markdown", "symfony markdown cms", "markdown content render"],
    "homepage": "https://github.com/maschmann/MarkdownContentBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Aschmann",
            "email": "maschmann@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/config": "^2.6 || ^3.0",
        "symfony/console": "^2.6 || ^3.0",
        "symfony/dependency-injection": "^2.6 || ^3.0",
        "symfony/filesystem": "^2.6 || ^3.0",
        "symfony/finder": "^2.6 || ^3.0",
        "symfony/yaml": "^2.6 || ^3.0"                                                                                ,
        "symfony/event-dispatcher": "^2.6 || ^3.0",
        "symfony/http-kernel": "^2.6 || ^3.0",
        "erusev/parsedown": "~1.6",
        "michelf/php-markdown": "~1.6"
    },
    "require-dev": {
        "symfony/framework-bundle": "^2.6 || ^3.0",
        "symfony/security-bundle": "^2.6 || ^3.0"
    },
    "config": {
        "disable-tls": true,
        "secure-http": false
    },
    "autoload": {
        "psr-4": {
            "Asm\\MarkdownContentBundle\\": ""
        }
    }
}
