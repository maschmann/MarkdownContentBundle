    "keywords": ["symfony markdown", "symfony markdown cms", "markdown content render"],
    "homepage": "https://github.com/maschmann/MarkdownContentBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Aschmann",
            "email": "maschmann@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/filesystem": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/yaml": "~2.3"                                                                                ,
        "symfony/event-dispatcher": "~2.3",
        "symfony/http-kernel": "~2.3",
        "erusev/parsedown": "~0.9",
        "michelf/php-markdown": "~1.4"
    },
    "require-dev": {
        "symfony/framework-bundle": "~2.3",
        "symfony/security-bundle": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Asm\\MarkdownContentBundle\\": ""
        }
    }
}
