{
    "name": "asm/markdown-content-bundle",
    "description": "Symfony2 bundle for static markdown-file content management",
    "keywords": ["symfony markdown", "symfony markdown cms", "markdown content render"],
    "homepage": "https://github.com/maschmann/MarkdownContentBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Aschmann",
            "email": "maschmann@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/config": "2.*",
        "symfony/console": "2.*",
        "symfony/dependency-injection": "2.*",
        "symfony/filesystem": "2.*",
        "symfony/finder": "2.*",
        "symfony/yaml": "2.*"                                                                                ,
        "symfony/event-dispatcher": "2.*",
        "symfony/http-kernel": "2.*",
        "erusev/parsedown": "~0.9",
        "michelf/php-markdown": "~1.4"
    },
    "require-dev": {
        "symfony/framework-bundle": "2.*",
        "symfony/security-bundle": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Asm\\MarkdownContentBundle\\": "src/"
        }
    }
}
