{
    "name": "asm/markdown-content-bundle",
    "description": "Symfony2 bundle for static markdown-file content management",
    "keywords": ["symfony markdown", "symfony markdown cms"],
    "homepage": "https://github.com/maschmann/MarkdownContentBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Aschmann",
            "email": "maschmann@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "doctrine/orm": "~2.2,>=2.2.3",
        "symfony/config": "~2.2",
        "symfony/console": "~2.2",
        "symfony/dependency-injection": "~2.2",
        "symfony/filesystem": "~2.2",
        "symfony/finder": "~2.2",
        "symfony/http-kernel": "~2.2",
        "symfony/translation": "~2.2",
        "erusev/parsedown": "~0.9",
        "michelf/php-markdown": "~1.4"
    },
    "require-dev": {
        "symfony/framework-bundle": "~2.2",
        "symfony/security-bundle": "~2.2"
    },
    "autoload": {
        "psr-0": {
            "Asm\\MarkdownContentBundle": ""
        }
    },
    "target-dir": "Asm/MarkdownContentBundle"
}
